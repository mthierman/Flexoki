            "scope": "markup.quote",
            "settings": {
                "foreground": "#116329"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "foreground": "#24292F",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "foreground": "#24292F",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["markup.underline"],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": ["markup.strikethrough"],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#0550AE"
            }
        },
        {
            "scope": [
                "markup.deleted",
                "meta.diff.header.from-file",
                "punctuation.definition.deleted"
            ],
            "settings": {
                "foreground": "#82071E",
                "background": "#FFEBE9"
            }
        },
        {
            "scope": ["punctuation.section.embedded"],
            "settings": {
                "foreground": "#CF222E"
            }
        },
        {
            "scope": [
                "markup.inserted",
                "meta.diff.header.to-file",
                "punctuation.definition.inserted"
            ],
            "settings": {
                "foreground": "#116329",
                "background": "#DAFBE1"
            }
        },
        {
            "scope": ["markup.changed", "punctuation.definition.changed"],
            "settings": {
                "foreground": "#953800",
                "background": "#FFD8B5"
            }
        },
        {
            "scope": ["markup.ignored", "markup.untracked"],
            "settings": {
                "foreground": "#EAEEF2",
                "background": "#0550AE"
            }
        },
        {
            "scope": "meta.diff.range",
            "settings": {
                "foreground": "#8250DF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#0550AE"
            }
        },
        {
            "scope": "meta.separator",
            "settings": {
                "foreground": "#0550AE",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.output",
            "settings": {
                "foreground": "#0550AE"
            }
        },
        {
            "scope": [
                "brackethighlighter.tag",
                "brackethighlighter.curly",
                "brackethighlighter.round",
                "brackethighlighter.square",
                "brackethighlighter.angle",
                "brackethighlighter.quote"
            ],
            "settings": {
                "foreground": "#57606A"
            }
        },
        {
            "scope": "brackethighlighter.unmatched",
            "settings": {
                "foreground": "#82071E"
            }
        },
        {
            "scope": ["constant.other.reference.link", "string.other.link"],
            "settings": {
                "foreground": "#0A3069",
                "fontStyle": "underline"
            }
        },
        {
            "scope": ["entity.name.function.powershell"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["support.function.powershell"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["variable.other.readwrite.powershell"],
            "settings": {
                "foreground": "#A40E26"
            }
        },
        {
            "scope": ["keyword.control.powershell"],
            "settings": {
                "foreground": "#CF222E"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#316BCD"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#CD9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#CD3131"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#800080"
            }
        }
