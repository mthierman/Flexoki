{
    "$schema": "vscode://schemas/color-theme",
    "name": "Flexoki Light",
    "semanticHighlighting": true,
    "colors": {
        "titleBar.activeBackground": "#F2F0E5",
        "titleBar.inactiveBackground": "#FFFCF0",
        "titleBar.border": "#E6E4D9",
        "titleBar.activeForeground": "#100F0F",
        "titleBar.inactiveForeground": "#B7B5AC",
        "editor.background": "#FFFCF0",
        "editor.foreground": "#100F0F",
        "statusBar.background": "#F2F0E5",
        "statusBar.noFolderBackground": "#F2F0E5",
        "statusBar.debuggingBackground": "#CECDC3",
        "statusBar.border": "#E6E4D9",
        "statusBar.noFolderBorder": "#E6E4D9",
        "statusBar.debuggingBorder": "#E6E4D9",
        "statusBar.focusBorder": "#E6E4D9",
        "statusBar.foreground": "#100F0F",
        "statusBar.debuggingForeground": "#100F0F",
        "statusBar.noFolderForeground": "#100F0F",
        "sideBar.background": "#F2F0E5",
        "sideBar.border": "#E6E4D9",
        "sideBar.foreground": "#100F0F",
        "activityBar.background": "#F2F0E5",
        "activityBar.border": "#E6E4D9",
        "activityBar.activeBackground": "#E6E4D9",
        "activityBar.activeBorder": "#FFFFFF00",
        "activityBar.activeFocusBorder": "#FFFFFF00",
        "activityBar.dropBorder": "#FFFFFF00",
        "activityBar.foreground": "#100F0F",
        "activityBar.inactiveForeground": "#6F6E69",
        "activityBarBadge.background": "#CE5D97",
        "activityBarBadge.foreground": "#100F0F",
        "commandCenter.background": "#F2F0E5",
        "commandCenter.border": "#E6E4D9",
        "commandCenter.foreground": "#6F6E69",
        "commandCenter.inactiveForeground": "#B7B5AC",
        "commandCenter.inactiveBorder": "#E6E4D9",
        "commandCenter.activeBorder": "#E6E4D9",
        "commandCenter.activeBackground": "#FFFCF0",
        "commandCenter.activeForeground": "#100F0F",
        "commandCenter.debuggingBackground": "#FFFCF0",
        "quickInput.background": "#FFFCF0",
        "quickInput.foreground": "#100F0F",
        "quickInputList.focusBackground": "#CECDC3",
        "quickInputList.focusForeground": "#100F0F",
        "quickInputList.focusIconForeground": "#100F0F",
        "list.hoverBackground": "#DAD8CE",
        "input.background": "#FFFCF0",
        "input.border": "#E6E4D9",
        "input.foreground": "#100F0F",
        "input.placeholderForeground": "#6F6E69",
        "menu.background": "#F2F0E5",
        "menu.border": "#FFFFFF00",
        "menu.foreground": "#100F0F",
        "menu.selectionBackground": "#DAD8CE",
        "menu.selectionBorder": "#FFFFFF00",
        "menu.selectionForeground": "#100F0F",
        "menu.separatorBackground": "#E6E4D9",
        "menubar.selectionBackground": "#DAD8CE",
        "menubar.selectionBorder": "#FFFFFF00",
        "menubar.selectionForeground": "#100F0F",
        "panel.background": "#F2F0E5",
        "panel.border": "#E6E4D9",
        "button.background": "#E6E4D9",
        "button.border": "#FFFFFF00",
        "button.foreground": "#100F0F",
        "button.hoverBackground": "#DAD8CE",
        "dropdown.background": "#FFFCF0",
        "dropdown.border": "#E6E4D9",
        "dropdown.foreground": "#100F0F",
        "dropdown.listBackground": "#FFFCF0",
        "editorCursor.background": "#CE5D97",
        "editorCursor.foreground": "#CE5D97",
        "editorGroupHeader.tabsBackground": "#FFFCF0",
        "editorGroup.border": "#E6E4D9",
        "editorGroupHeader.border": "#E6E4D9",
        "tab.activeForeground": "#100F0F",
        "tab.activeBackground": "#F2F0E5",
        "tab.inactiveBackground": "#FFFCF0",
        "tab.hoverBackground": "#E6E4D9",
        "tab.border": "#E6E4D9",
        "settings.focusedRowBorder": "#FFFFFF00",
        "focusBorder": "#FFFFFF00",
        "list.activeSelectionBackground": "#CECDC3",
        "list.inactiveSelectionBackground": "#E6E4D9",
        "list.activeSelectionForeground": "#100F0F",
        "list.inactiveSelectionForeground": "#100F0F",
        "settings.focusedRowBackground": "#F2F0E5",
        "settings.rowHoverBackground": "#F2F0E5",
        "editorWidget.background": "#E6E4D9",
        "editorWidget.foreground": "#100F0F",
        "editorWidget.border": "#E6E4D9",
        "editorSuggestWidget.background": "#F2F0E5",
        "icon.foreground": "#100F0F",
        "terminal.border": "#E6E4D9",
        "terminal.background": "#F2F0E5",
        "terminal.ansiBlack": "#100F0F",
        "terminal.ansiBlue": "#4385BE",
        "terminal.ansiBrightBlack": "#575653",
        "terminal.ansiBrightBlue": "#205EA6",
        "terminal.ansiBrightCyan": "#24837B",
        "terminal.ansiBrightGreen": "#66800B",
        "terminal.ansiBrightMagenta": "#A02F6F",
        "terminal.ansiBrightRed": "#AF3029",
        "terminal.ansiBrightWhite": "#FFFCF0",
        "terminal.ansiBrightYellow": "#AD8301",
        "terminalCursor.foreground": "#100F0F",
        "terminal.ansiCyan": "#3AA99F",
        "terminal.foreground": "#100F0F",
        "terminal.ansiGreen": "#879A39",
        "terminal.ansiMagenta": "#CE5D97",
        "terminal.ansiRed": "#D14D41",
        "terminal.selectionBackground": "#DA702C",
        "terminal.ansiWhite": "#B7B5AC",
        "terminal.ansiYellow": "#D0A215"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment.block",
                "comment.block.documentation",
                "comment.line"
            ],
            "settings": {
                "foreground": "#B7B5AC"
            }
        },
        {
            "scope": [
                "constant",
                "constant.character",
                "constant.character.escape",
                "constant.other",
                "constant.regexp",
                "constant.rgb-value",
                "support.constant"
            ],
            "settings": {
                "foreground": "#AD8301"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.integer",
                "constant.numeric.octal"
            ],
            "settings": {
                "foreground": "#5E409D"
            }
        },
        {
            "scope": [
                "entity",
                "entity.name",
                "entity.name.class",
                "entity.name.function",
                "entity.name.method",
                "entity.name.section",
                "entity.name.selector",
                "entity.name.tag",
                "entity.name.type",
                "entity.other",
                "entity.other.attribute-name",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#BC5215"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.deprecated",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#AF3029"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.other",
                "string.json"
            ],
            "settings": {
                "foreground": "#66800B"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.logical",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#6F6E69"
            }
        },
        {
            "scope": [
                "meta",
                "meta.block",
                "meta.cast",
                "meta.class",
                "meta.function",
                "meta.function-call",
                "meta.preprocessor",
                "meta.return-type",
                "meta.selector",
                "meta.tag",
                "meta.type",
                "meta.type.annotation"
            ],
            "settings": {
                "foreground": "#A02F6F"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "punctuation.separator",
                "punctuation.separator.continuation",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#6F6E69"
            }
        },
        {
            "scope": [
                "storage",
                "storage.modifier",
                "storage.type"
            ],
            "settings": {
                "foreground": "#66800B"
            }
        },
        {
            "scope": [
                "string",
                "string.interpolated",
                "string.other",
                "string.regexp",
                "string.quoted",
                "string.quoted.double",
                "string.quoted.other",
                "string.quoted.single",
                "string.quoted.triple",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#24837B"
            }
        },
        {
            "scope": [
                "variable",
                "variable.language",
                "variable.name",
                "variable.other",
                "variable.other.readwrite",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#205EA6"
            }
        }
    ]
}