{
    "$schema": "vscode://schemas/color-theme",
    "name": "Flexoki Dark",
    "semanticHighlighting": true,
    "colors": {
        "titleBar.activeBackground": "#1C1B1A",
        "titleBar.inactiveBackground": "#100F0F",
        "titleBar.border": "#282726",
        "titleBar.activeForeground": "#CECDC3",
        "titleBar.inactiveForeground": "#575653",
        "editor.background": "#100F0F",
        "editor.foreground": "#CECDC3",
        "statusBar.background": "#100F0F",
        "statusBar.noFolderBackground": "#100F0F",
        "statusBar.debuggingBackground": "#A02F6F",
        "statusBar.border": "#282726",
        "statusBar.noFolderBorder": "#282726",
        "statusBar.debuggingBorder": "#282726",
        "statusBar.focusBorder": "#282726",
        "statusBar.foreground": "#CECDC3",
        "statusBar.debuggingForeground": "#CECDC3",
        "statusBar.noFolderForeground": "#CECDC3",
        "sideBar.background": "#1C1B1A",
        "sideBar.border": "#282726",
        "sideBar.foreground": "#CECDC3",
        "activityBar.background": "#100F0F",
        "activityBar.border": "#282726",
        "activityBar.activeBackground": "#282726",
        "activityBar.activeBorder": "#A02F6F",
        "activityBar.activeFocusBorder": "#A02F6F",
        "activityBar.dropBorder": "#A02F6F",
        "activityBar.foreground": "#CECDC3",
        "activityBar.inactiveForeground": "#878580",
        "activityBarBadge.background": "#A02F6F",
        "activityBarBadge.foreground": "#CECDC3",
        "commandCenter.background": "#100F0F",
        "commandCenter.border": "#282726",
        "commandCenter.foreground": "#878580",
        "commandCenter.inactiveForeground": "#575653",
        "commandCenter.inactiveBorder": "#282726",
        "commandCenter.activeBorder": "#282726",
        "commandCenter.activeBackground": "#100F0F",
        "commandCenter.activeForeground": "#CECDC3",
        "commandCenter.debuggingBackground": "#100F0F",
        "quickInput.background": "#100F0F",
        "quickInput.foreground": "#CECDC3",
        "quickInputList.focusBackground": "#A02F6F",
        "quickInputList.focusForeground": "#CECDC3",
        "quickInputList.focusIconForeground": "#CECDC3",
        "input.background": "#100F0F",
        "input.border": "#282726",
        "input.foreground": "#CECDC3",
        "input.placeholderForeground": "#878580",
        "menu.background": "#100F0F",
        "menu.border": "#FFFFFF00",
        "menu.foreground": "#CECDC3",
        "menu.selectionBackground": "#A02F6F",
        "menu.selectionBorder": "#A02F6F",
        "menu.selectionForeground": "#CECDC3",
        "menu.separatorBackground": "#282726",
        "menubar.selectionBackground": "#A02F6F",
        "menubar.selectionBorder": "#FFFFFF00",
        "menubar.selectionForeground": "#CECDC3",
        "panel.background": "#1C1B1A",
        "panel.border": "#282726",
        "button.background": "#A02F6F",
        "button.border": "#282726",
        "button.foreground": "#CECDC3",
        "button.hoverBackground": "#CE5D97",
        "dropdown.background": "#100F0F",
        "dropdown.border": "#282726",
        "dropdown.foreground": "#CECDC3",
        "dropdown.listBackground": "#100F0F",
        "editorCursor.background": "#A02F6F",
        "editorCursor.foreground": "#A02F6F",
        "editorGroupHeader.tabsBackground": "#100F0F",
        "editorGroup.border": "#282726",
        "editorGroupHeader.border": "#282726",
        "tab.activeForeground": "#CECDC3",
        "tab.activeBackground": "#1C1B1A",
        "tab.inactiveBackground": "#100F0F",
        "tab.hoverBackground": "#282726",
        "tab.border": "#282726",
        "settings.focusedRowBorder": "#A02F6F",
        "focusBorder": "#A02F6F",
        "settings.focusedRowBackground": "#1C1B1A",
        "settings.rowHoverBackground": "#1C1B1A",
        "terminal.border": "#282726",
        "terminal.background": "#1C1B1A",
        "terminal.ansiBlack": "#100F0F",
        "terminal.ansiBlue": "#205EA6",
        "terminal.ansiBrightBlack": "#575653",
        "terminal.ansiBrightBlue": "#4385BE",
        "terminal.ansiBrightCyan": "#3AA99F",
        "terminal.ansiBrightGreen": "#879A39",
        "terminal.ansiBrightMagenta": "#CE5D97",
        "terminal.ansiBrightRed": "#D14D41",
        "terminal.ansiBrightWhite": "#FFFCF0",
        "terminal.ansiBrightYellow": "#D0A215",
        "terminalCursor.foreground": "#CECDC3",
        "terminal.ansiCyan": "#24837B",
        "terminal.foreground": "#CECDC3",
        "terminal.ansiGreen": "#66800B",
        "terminal.ansiMagenta": "#A02F6F",
        "terminal.ansiRed": "#AF3029",
        "terminal.selectionBackground": "#BC5215",
        "terminal.ansiWhite": "#B7B5AC",
        "terminal.ansiYellow": "#AD8301"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment.block",
                "comment.block.documentation",
                "comment.line"
            ],
            "settings": {
                "foreground": "#575653"
            }
        },
        {
            "scope": [
                "constant",
                "constant.character",
                "constant.character.escape",
                "constant.other",
                "constant.regexp",
                "constant.rgb-value",
                "support.constant"
            ],
            "settings": {
                "foreground": "#D0A215"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.integer",
                "constant.numeric.octal"
            ],
            "settings": {
                "foreground": "#8B7EC8"
            }
        },
        {
            "scope": [
                "entity",
                "entity.name",
                "entity.name.class",
                "entity.name.function",
                "entity.name.method",
                "entity.name.section",
                "entity.name.selector",
                "entity.name.tag",
                "entity.name.type",
                "entity.other",
                "entity.other.attribute-name",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#DA702C"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.deprecated",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#D14D41"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.other",
                "string.json"
            ],
            "settings": {
                "foreground": "#879A39"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.logical",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#878580"
            }
        },
        {
            "scope": [
                "meta",
                "meta.block",
                "meta.cast",
                "meta.class",
                "meta.function",
                "meta.function-call",
                "meta.preprocessor",
                "meta.return-type",
                "meta.selector",
                "meta.tag",
                "meta.type",
                "meta.type.annotation"
            ],
            "settings": {
                "foreground": "#CE5D97"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "punctuation.separator",
                "punctuation.separator.continuation",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#878580"
            }
        },
        {
            "scope": [
                "storage",
                "storage.modifier",
                "storage.type"
            ],
            "settings": {
                "foreground": "#879A39"
            }
        },
        {
            "scope": [
                "string",
                "string.interpolated",
                "string.other",
                "string.regexp",
                "string.quoted",
                "string.quoted.double",
                "string.quoted.other",
                "string.quoted.single",
                "string.quoted.triple",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#3AA99F"
            }
        },
        {
            "scope": [
                "variable",
                "variable.language",
                "variable.name",
                "variable.other",
                "variable.other.readwrite",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#4385BE"
            }
        }
    ]
}