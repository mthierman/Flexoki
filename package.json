{
    "name": "theme-custom",
    "displayName": "%display%",
    "description": "%description%",
    "categories": [
        "Themes"
    ],
    "version": "1.0.0",
    "publisher": "Mike Thierman",
    "license": "MIT",
    "engines": {
        "vscode": "*"
    },
    "contributes": {
        "themes": [
            {
                "id": "%light%",
                "label": "%light%",
                "uiTheme": "vs",
                "path": "./themes/modern-light-color-theme.json"
            },
            {
                "id": "%dark%",
                "label": "%dark%",
                "uiTheme": "vs-dark",
                "path": "./themes/modern-dark-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "generator-code": "latest",
        "yo": "latest"
    }
}
