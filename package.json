{
    "name": "theme-custom",
    "displayName": "%display%",
    "description": "%description%",
    "categories": [
        "Themes"
    ],
    "version": "0.0.1",
    "publisher": "Mike Thierman",
    "license": "MIT",
    "engines": {
        "vscode": "*"
    },
    "contributes": {
        "themes": [
            {
                "id": "%dark%",
                "label": "%dark%",
                "uiTheme": "vs-dark",
                "path": "./themes/flexoki-dark-color-theme.json"
            },
            {
                "id": "%light%",
                "label": "%light%",
                "uiTheme": "vs",
                "path": "./themes/flexoki-light-color-theme.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mthierman/vscode-themes.git"
    },
    "devDependencies": {
        "generator-code": "latest",
        "yo": "latest"
    }
}
